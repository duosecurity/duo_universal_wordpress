{
  "name": "duosecurity/duo_universal_wordpress",
  "description": "Duo Universal WordPress",
  "homepage": "https://github.com/duosecurity/duo_universal_wordpress",
  "type": "wordpress-plugin",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Duo Security",
      "homepage": "http://www.duosecurity.com/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "duosecurity/duo_universal_php": ">=1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
